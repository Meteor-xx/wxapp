.page__ft{
  display: flex;
  justify-content: center;
}

.user{
  display: flex;
  flex-direction:column;
  align-items: center;
  z-index: 2;
  height: 100%;
  width:100%;
  position: absolute;
  padding-right: 415rpx;
}
.userinfo-avatar {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
  margin: 20rpx;
}

.userinfo-nickname {
  color: #000;
  display:block;
  width: 240rpx;
  font-size:40rpx;
  overflow:hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  text-align:center;
}
.container {
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding-right: 40rpx;
  padding-left: 40rpx;
  padding-top: 40rpx;
  padding-bottom: 0;
  box-sizing: border-box;
  background-color: #00aeff;
  z-index: 0

} 
.box1{
  background-color:white;
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
  border-top-left-radius: 9px;
  border-top-right-radius: 9px;
  z-index:1; 
  text-align: right;
  display: flex;
  flex-direction: column;
  border-bottom-color: rgb(0, 213, 228);
  border-bottom-style: solid;
}
.box2{
  background-color: white;
  height: 160rpx;
  margin-right: 40rpx;
  margin-left: 40rpx;
  border-bottom-left-radius: 9px;
  border-bottom-right-radius: 9px;
  padding-top: 15rpx;
  display: flex;
  flex-direction: row;
  box-shadow: 0 3px 9px 1px rgba(146,146,146,0.402);
}
.box2-cell{
    display: flex;
    flex-direction: column;
    flex: 1;
    text-align: center;

}
.sticker{
  padding: 40rpx;
  padding-bottom: 0;
  z-index: 2;
  text-align: left;
  display: flex;
  flex-direction: column
}
.sticker_bd{
  background-color: white;
  height: 380rpx;
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  border-bottom-right-radius: 5px;
  border-bottom-left-radius: 5px;
  box-shadow: 0 3px 6px 1px rgba(146,146,146,0.402);
  z-index: 3;
  
}
.sticker_cot{
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.sticker_line{
  text-align: left;
  display: flex;
  flex-direction: column;
  font-size: 40rpx;
  /* padding: 15rpx; */
margin-left: 10px;
}
.deleter{
  height:70rpx;
  display:flex;
  flex-direction:row-reverse;
  margin-right:50rpx;
  margin-top: 45rpx;
  z-index: 2
}
.deleter_bd{
  display:flex;
  flex-direction:row;
  color:rgba(146, 146, 146, 0.452);
  padding: 1px;
  height: 70rpx;
  width: 140rpx;
  border-radius:15px;
  background-color:white;
  justify-content: center;
  align-items: center;
  text-align: center;
  z-index: 2;
  box-shadow: 0 0 6px 1px rgba(146,146,146,0.402)
}
.deleter_cell{
  display: flex;
  margin: 1px;
  justify-content: center;
  align-items: center;
  text-align: center;
  z-index: 3
}